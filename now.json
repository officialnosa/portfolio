{
  "name": "portfolio",
  "rewrites": [
    {
      "source": "/apps/aplus/(.*)",
      "destination": "https://aplus.osarogie.com/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "version": 2
}
