{
  "name": "portfolio",
  "routes": [
    {
      "src": "^/service-worker.js$",
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    }
  ],
  "rewrites": [
    { "source": "/apps/aplus/(.*)", "destination": "https://aplus.osarogie.com/$1" }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "version": 2
}
